#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    int n1, n2, i = 0, j = 0;
    scanf("%d", &n1);

    int a[n1];
    for(int k = 0; k < n1; k++)
        scanf("%d", &a[k]);

    scanf("%d", &n2);
    int b[n2];
    for(int k = 0; k < n2; k++)
        scanf("%d", &b[k]);

    int lastPrinted = -1;  

    while(i < n1 && j < n2) {
        int value;
        if(a[i] < b[j])
            value = a[i++];
        else if(a[i] > b[j])
            value = b[j++];
        else {
            value = a[i];
            i++;
            j++;
        }

        if(value != lastPrinted) {
            printf("%d ", value);
            lastPrinted = value;
        }
    }

    while(i < n1) {
        if(a[i] != lastPrinted) {
            printf("%d ", a[i]);
            lastPrinted = a[i];
        }
        i++;
    }

    while(j < n2) {
        if(b[j] != lastPrinted) {
            printf("%d ", b[j]);
            lastPrinted = b[j];
        }
        j++;
    }

    return 0;
}
